{
    "name": "Chrome URL Tracker",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Display Chrome URL entered by user",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background":{
        "scripts": ["background.js"]
    },
    "permissions": ["storage", 
            "tabs", "activeTab",
        "webRequest", "*://*.qbo.intuit.com/"]
}
